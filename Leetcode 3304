class Solution {
public:
    char kthCharacter(int k) {
        string s="a";
        while(s.size()<k){
            string temp =s;
            for(int i=0;i<s.size();i++){
                temp[i]=((temp[i]=='z')?'a':temp[i]+1);
            }
            s+=temp;
        }
        return s[k-1];
    }
};
