#include <iostream>
#include<algorithm>
#include <cmath>
#include<iomanip>
using namespace std;

int main() {
	long long n;
	cin >> n;
	int count = 0;
	double rootn = sqrt(n);
	for (int i = 1; i <= sqrt(n); i++) {
		if (n % i == 0) {
			if (i == n / i) {
				count++;
			}
			else 
			count += 2;
		}
	}
	
	if (count % 2 == 0) {
		cout << "NO" << endl;
	}
	else {
		cout << "YES" << endl;
	}
}
